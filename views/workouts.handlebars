workouts 

<section>
    <h2>My Workouts</h2>
    <p>Your workouts will show up here</p>
    <button id="create-button">Create New Workout</button>
</section>
<div class="modal" id="modal">
    <div class="modal-background"></div>
    <div class="modal-card">
        <header class="modal-card-head">
            <p class="modal-card-title">Create New Workout</p>
        </header>
        <section class="modal-card-body">
            <div class="field">
                <label class="label">Title:</label>
                <div class="control">
                    <input id="title-id" class="input" type="text" placeholder="Type here">
                </div>
            </div>
            <div class="field">
                <label class="label">Category:</label>
                <div class="control">
                    <div class="select">
                        <select id="category">
                            <option>Please select</option>
                            <option>Cardio</option>
                            <option>Stamina</option>
                            <option>Strength</option>
                            <option>Flexibility</option>
                            <option>Coordination</option>
                            <option>Agility</option>
                            <option>Balance</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="field">
                <label class="label">Description:</label>
                <div class="control">
                    <input id="description-id" class="input" type="Type here" placeholder="">
                </div>
            </div>
        </section>
        <footer class="modal-card-foot">
            <button class="button is-success" id="save-button">Save Workout</button>
            <button class="button is-danger" id="cancel-button">Cancel</button>
        </footer>
    </div>
</div>
<section>
    {{#each workout-card as |workout|}}
    {{/each}}
</section>

<script src="/public/js/newWorkout.js"></script>